#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ace/g20/energy_mall/node_modules/.pnpm/esno@4.8.0/node_modules/esno/node_modules:/Users/ace/g20/energy_mall/node_modules/.pnpm/esno@4.8.0/node_modules:/Users/ace/g20/energy_mall/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ace/g20/energy_mall/node_modules/.pnpm/esno@4.8.0/node_modules/esno/node_modules:/Users/ace/g20/energy_mall/node_modules/.pnpm/esno@4.8.0/node_modules:/Users/ace/g20/energy_mall/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esno/esno.js" "$@"
else
  exec node  "$basedir/../esno/esno.js" "$@"
fi
